'use strict';

(function(){

  angular.module('TravelBlog', [
    'ngRoute',
    'mm.foundation',
    'TravelBlog.filters',
    'TravelBlog.services',
    'TravelBlog.directives',
    'TravelBlog.controllers',
    'google-maps'
  ]).

  config(['$routeProvider', function($routeProvider) {
    $routeProvider.when('/', {templateUrl: '<%= asset_path("layout.html") %>', controller: 'IndexCtrl'});
    $routeProvider.otherwise({redirectTo: '/'});
  }]);

})();